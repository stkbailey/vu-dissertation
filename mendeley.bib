doi = {10.1016/j.neuroimage.2013.06.072},
eprint = {NIHMS150003},
file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Go{\~{n}}i et al. - 2013 - Robust estimation of fractal measures for characterizing the structural complexity of the human brain Optimization.pdf:pdf},
