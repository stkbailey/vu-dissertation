journal = {Mind, Brain, and Education},
number = {3},
pages = {140--149},
title = {{How Educational Theories Can Use Neuroscientific Data}},
url = {http://doi.wiley.com/10.1111/j.1751-228X.2007.00014.x},
volume = {1},
year = {2007}
}
@article{Goswami2002,
abstract = {In this paper, I present a theoretical overview at the cognitive level of the role of phonological awareness in reading development and devel- opmental dyslexia across languages. My assumption is that the pri- mary deficit in developmental dyslexia in all languages lies in representing speech sounds: a deficit in "phonological representation." I will argue that this deficit manifests in somewhat different ways, de- pending on orthography. I will also argue that the phonological deficit in dyslexia is initially at the syllable and onset-rime levels of phono- logical awareness, with the development of "phonemic" awareness being a consequence rather than a precursor of reading. Finally, I will suggest that some of the processes underpinning language acquisition are disrupted in dyslexia, in particular, the detection of rhythm in speech.},
