isbn = {1522-2594 (Electronic)$\backslash$n0740-3194 (Linking)},
issn = {07403194},
journal = {Magnetic Resonance in Medicine},
keywords = {MRI,chemical exchange,contrast agent,dipolar transfer,magnetization transfer},
month = {apr},
number = {4},
pages = {927--948},
pmid = {21337419},
title = {{Chemical exchange saturation transfer (CEST): What is in a name and what isn't?}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3148076{\&}tool=pmcentrez{\&}rendertype=abstract},
