@article{Nicolson2001,
abstract = {Surprisingly, the problems faced by many dyslexic children are by no means confined to reading and spelling. There appears to be a general impairment in the ability to perform skills automatically, an ability thought to be dependent upon the cerebellum. Specific behavioural and neuroimaging tests reviewed here indicate that dyslexia is indeed associated with cerebellar impairment in about 80{\%} of cases. We propose that disorders of cerebellar development can in fact cause the impairments in reading and writing characteristic of dyslexia, a view consistent with the recently appreciated role of the cerebellum in language-related skills. This proposal has implications for early remedial treatment.},
annote = {Two main hypotheses re: the origin of dyslexia attract most attention: the phonological deficit account, which holds that reading difficulties derive from problems in breaking down spoken words in phonemes, and the magnocellular deficit account, which holds the impaired sensory processing, esp. in auditory and/or visual magnocellular pathways prevent skilled reading acquisition. However, functional studies and behavioral studies show that cerebellar abnormalities also exist. The cerebellum plays a role in automatising skill and knowledge, balance / motor, and articulation. Impairments of these could create downstream problems in reading, spelling and writing. Specifically, studies have shown that dyslexic children and adults have muscle dystonia, less functional activations in cerebellar tasks (e.g. learning and practicing a sequence of button presses), and abnormalities in MR spectroscopic measures. This cerebellar deficit hypothesis is a plausible, biological explanation that may co-occur with the magnocellular deficits also observed.},
author = {Nicolson, Roderick I. and Fawcett, Angela J. and Dean, Paul},
doi = {10.1016/S0166-2236(00)01896-8},
file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Nicolson, Fawcett, Dean - 2001 - Developmental dyslexia The cerebellar deficit hypothesis.pdf:pdf},
isbn = {0166-2236},
issn = {01662236},
journal = {Trends in Neurosciences},
number = {9},
pages = {508--511},
pmid = {11506881},
title = {{Developmental dyslexia: The cerebellar deficit hypothesis}},
volume = {24},
year = {2001}
}
@article{Nicolson2007,
abstract = {During the past 30 years, research into developmental disorders has fragmented, emphasizing differences rather than commonalities. We propose that reunification might be achieved by using a 'neural-systems' approach. Deficits in dyslexia are attributed to an intact declarative learning system combined with an impaired procedural learning system - a network that includes prefrontal language systems and basal ganglia, parietal and cerebellar structures. A typology is provided for other prevalent learning disabilities; this framework focuses on different learning skills in the understanding of learning disabilities and emphasizes the diagnostic significance of 'secondary' symptoms. This approach highlights the need for development of 'neurocognitive' tests to probe the function of components of each neural system and improve strategies for explanation, diagnosis and support of developmental disorders. ?? 2006 Elsevier Ltd. All rights reserved.},
author = {Nicolson, Roderick I. and Fawcett, Angela J.},
doi = {10.1016/j.tins.2007.02.003},
file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Nicolson, Fawcett - 2007 - Procedural learning difficulties reuniting the developmental disorders.pdf:pdf},
isbn = {0166-2236},
issn = {01662236},
journal = {Trends in Neurosciences},
keywords = {Brain,Brain: pathology,Brain: physiopathology,Child,Developmental Disabilities,Developmental Disabilities: complications,Developmental Disabilities: pathology,Humans,Learning Disorders,Learning Disorders: classification,Learning Disorders: complications,Learning Disorders: pathology,Nerve Net,Nerve Net: pathology,Nerve Net: physiopathology},
