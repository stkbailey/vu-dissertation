eprint = {NIHMS150003},
file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Turkeltaub et al. - 2012 - Left lateralizing transcranial direct current stimulation improves reading efficiency.pdf:pdf},
isbn = {1935-861X},
