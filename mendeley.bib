issn = {1939-2222},
journal = {Journal of Experimental Psychology: General},
keywords = {Adolescent,Adult,Electroencephalography,Electroencephalography: instrumentation,Electroencephalography: methods,Evoked Potentials,Eye Movements,Eye Movements: physiology,Female,Fixation,Humans,Male,Ocular,Ocular: physiology,Reading,Visual,Visual Perception,Visual Perception: physiology,Visual: physiology,Young Adult},
