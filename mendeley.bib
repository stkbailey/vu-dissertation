eprint = {0505185},
file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Iturria-Medina et al. - 2008 - Studying the human brain anatomical network via diffusion-weighted MRI and Graph Theory.pdf:pdf},
isbn = {1053-8119 (Print)$\backslash$n1053-8119 (Linking)},
