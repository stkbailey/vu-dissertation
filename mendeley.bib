@article{Eliez2000,
abstract = {Functional imaging studies of developmental dyslexia have reported reduced task-related neural activity in the temporal and inferior parietal cortices. To examine the possible contribution of subtle anatomic deviations to these reductions, volumes were measured for the major lobes of the brain, the subcortical nuclei, cerebellum, and lateral ventricles on magnetic resonance imaging (MRI) scans from 16 right-handed dyslexic men, ages 18 to 40, and 14 matched controls, most of whom had previously undergone PET imaging. A specific decrease in tissue volume was localized to the temporal lobes and was particularly prominent on the left (p {\textless} .01). An analysis of tissue composition revealed that this reduction was primarily attributable to decreased gray matter within the left temporal lobe (p {\textless} .002). Further segmentation of the temporal lobe showed that this reduction was not confined to the superior temporal gyrus, the primary location of primary auditory cortex. Reductions of temporal lobe gray matter may reflect a regional decrease in neuronal number or neuropil, which in turn may result in reading impairment.},
author = {Eliez, Stephan and Rumsey, Judith M. and Giedd, Jay N. and Schmitt, J. Eric and Patwardhan, Anil J. and Reiss, Allan L.},
