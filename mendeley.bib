isbn = {1095-9572 (Electronic)$\backslash$r1053-8119 (Linking)},
issn = {10959572},
journal = {NeuroImage},
keywords = {Bayesian,Criticality,Dynamic causal modelling,Effective connectivity,FMRI,Free energy,Functional connectivity,Lyapunov exponents,Proximity graph,Resting state,Self-organisation},
pages = {533--547},
pmid = {24862075},
publisher = {Elsevier B.V.},
title = {{On nodes and modes in resting state fMRI}},
