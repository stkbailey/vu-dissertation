journal = {NeuroImage},
keywords = {Adult,Algorithms,Brain,Brain: anatomy {\&} histology,Diffusion Magnetic Resonance Imaging,Humans,Image Processing, Computer-Assisted,Male,Nerve Fibers,Nerve Fibers: physiology,Nerve Net,Nerve Net: anatomy {\&} histology,Neural Pathways,Neural Pathways: anatomy {\&} histology,Neural Pathways: physiology},
