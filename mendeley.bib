file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Wen et al. - 2011 - Discrete Neuroanatomical Networks Are Associated with Specific Cognitive Abilities in Old Age.pdf:pdf},
isbn = {1529-2401 (Electronic)$\backslash$r0270-6474 (Linking)},
issn = {0270-6474},
journal = {Journal of Neuroscience},
keywords = {80 and over,Aged,Aging,Aging: physiology,Brain Mapping,Cerebral Cortex,Cerebral Cortex: physiology,Cognition,Cognition: physiology,Diffusion Tensor Imaging,Female,Humans,Male,Nerve Net,Nerve Net: physiology},
