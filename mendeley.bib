file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Vermeij et al. - 2014 - Very-low-frequency oscillations of cerebral hemodynamics and blood pressure are affected by aging and cognitive.pdf:pdf},
isbn = {1053-8119},
issn = {10538119},
journal = {NeuroImage},
keywords = {BOLD,Cerebral autoregulation,Near infrared spectroscopy,Spontaneous oscillations,Vasomotion,Working memory},
