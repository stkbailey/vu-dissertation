issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences},
keywords = {Adolescent,Brain,Brain: anatomy {\&} histology,Brain: physiology,Child,Connectome,Diffusion Tensor Imaging,Female,Humans,Linear Models,Male,Sex Characteristics,Young Adult},
month = {jan},
