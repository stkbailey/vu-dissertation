file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Rimrodt et al. - 2010 - White matter microstructural differences linked to left perisylvian language network in children with dyslexia.pdf:pdf},
isbn = {1973-8102 (Electronic)$\backslash$r0010-9452 (Linking)},
issn = {00109452},
journal = {Cortex},
keywords = {Diffusion tensor imaging,Dyslexia,Fractional anisotropy,Left inferior frontal gyrus,Perisylvian language network},
month = {jun},
number = {6},
pages = {739--749},
pmid = {19682675},
publisher = {Elsevier Srl},
title = {{White matter microstructural differences linked to left perisylvian language network in children with dyslexia}},
