isbn = {9780192880512},
issn = {0898-929X},
journal = {Journal of Cognitive Neuroscience},
keywords = {Adolescent,Adult,Brain,Brain Mapping,Brain Mapping: instrumentation,Brain Mapping: methods,Brain: physiology,Female,Humans,Magnetic Resonance Imaging,Magnetic Resonance Imaging: instrumentation,Magnetic Resonance Imaging: methods,Male,Nerve Net,Nerve Net: physiology,Neuropsychological Tests,Young Adult},
