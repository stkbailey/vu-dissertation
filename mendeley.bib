keywords = {106-C,Alzheimer disease,Canavan disease,Huntington disease,MR Spectroscopy - compulsory,MRI,MRS,N-acetyl aspartate,biomedical imaging,brain disease,magnetic resonance imaging,myelination,neurochemistry,spectroscopy},
