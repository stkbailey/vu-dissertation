issn = {00340553},
journal = {Reading Research Quarterly},
keywords = {Adolescence,Comprehension,Comprehension monitoring,Depth of (higher level, literal level, etc.),Discourse analysis,Early adolescence,Experimental, quasi-experimental,Information processing,Informational text,Making inferences,Metacognition,Reading strategies,Research methodology,Strategies, methods, and materials,Struggling learners},
number = {4},
pages = {393--416},
title = {{Text-processing differences in adolescent adequate and poor comprehenders reading accessible and challenging narrative and informational text}},
