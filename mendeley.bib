@article{Harrar2014,
abstract = {Summary Developmental dyslexia affects 5{\%}-10{\%} of the population [1], resulting in poor spelling and reading skills. While there are well-documented differences in the way dyslexics process low-level visual [2, 3] and auditory [4, 5] stimuli, it is mostly unknown whether there are similar differences in audiovisual multisensory processes. Here, we investigated audiovisual integration using the redundant target effect (RTE) paradigm. Some conditions demonstrating audiovisual integration appear to depend upon magnocellular pathways [6], and dyslexia has been associated with deficits in this pathway [7]; so, we postulated that developmental dyslexics ("dyslexics" hereafter) would show differences in audiovisual integration compared with controls. Reaction times (RTs) to multisensory stimuli were compared with predictions from Miller's race model [8, 9]. Dyslexics showed difficulty shifting their attention between modalities; but such "sluggish attention shifting" (SAS) [10] appeared only when dyslexics shifted their attention from the visual to the auditory modality. These results suggest that dyslexics distribute their crossmodal attention resources differently from controls, causing different patterns in multisensory responses compared to controls. From this, we propose that dyslexia training programs should take into account the asymmetric shifts of crossmodal attention. {\textcopyright} 2014 Elsevier Ltd.},
author = {Harrar, Vanessa and Tammam, Jonathan and P{\'{e}}rez-Bellido, Alexis and Pitt, Anna and Stein, John and Spence, Charles},
doi = {10.1016/j.cub.2014.01.029},
file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Harrar et al. - 2014 - Multisensory integration and attention in developmental dyslexia.pdf:pdf},
isbn = {0960-9822},
issn = {09609822},
journal = {Current Biology},
month = {feb},
number = {5},
pages = {531--535},
pmid = {24530067},
title = {{Multisensory integration and attention in developmental dyslexia}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0960982214000621},
volume = {24},
year = {2014}
}
