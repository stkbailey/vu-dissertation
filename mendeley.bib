journal = {NeuroImage},
keywords = {Adolescent,Adult,Brain,Brain Mapping,Brain: physiology,Computer-Assisted,Humans,Image Processing,Magnetic Resonance Imaging,Male,Pattern Recognition,Reading,Speech,Speech Perception,Speech Perception: physiology,Speech: physiology,Visual,Visual: physiology},
