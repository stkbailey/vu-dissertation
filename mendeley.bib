month = {nov},
number = {45},
pages = {11435--11444},
pmid = {18987180},
title = {{Dissociating the Human Language Pathways with High Angular Resolution Diffusion Fiber Tractography}},
url = {http://www.jneurosci.org/cgi/doi/10.1523/JNEUROSCI.2388-08.2008},
