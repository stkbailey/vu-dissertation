isbn = {0028-3932 (Print)$\backslash$r0028-3932 (Linking)},
issn = {00283932},
journal = {Neuropsychologia},
keywords = {Dyslexia,Frontal lobe,MRI,Phonological processing,Temporal lobe},
month = {jan},
number = {3},
pages = {324--331},
pmid = {15707610},
title = {{Gray matter alteration in dyslexia: Converging evidence from volumetric and voxel-by-voxel MRI analyses}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15707610},
volume = {43},
year = {2005}
}
@article{Steinbrink2008,
abstract = {Developmental dyslexia is one of the most common neuropsychological disorders in children and adults. Only few data are available on the pathomechanisms of this specific dysfunction, assuming - among others - that dyslexia might be a disconnection syndrome of anterior and posterior brain regions involved in phonological and orthographic aspects of the reading process, as well as in the integration of phonemes and graphemes. Therefore, diffusion tensor imaging (DTI) and voxel-based morphometry (VBM) were used to verify the hypothesis of altered white and gray matter structure in German dyslexic adults. DTI revealed decreased fractional anisotropy (FA) in bilateral fronto-temporal and left temporo-parietal white matter regions (inferior and superior longitudinal fasciculus). Significant correlations between white matter anisotropy and speed of pseudoword reading were found. In dyslexics, gray matter volumes (as measured by VBM) were reduced in the superior temporal gyrus of both hemispheres. So far, our results, based on a combined analysis of white and gray matter abnormalities, provide exceedingly strong evidence for a disconnection syndrome or dysfunction of cortical areas relevant for reading and spelling. Thus, we suggest that this imbalance of neuronal communication between the respective brain areas might be the crucial point for the development of dyslexia. {\textcopyright} 2008 Elsevier Ltd. All rights reserved.},
annote = {Steinbrink et al. used VBM and DTI in German dyslexic adults. DTI revealed decreased FA in bilateral fronto-temporal and left temporo-parietal white matter regions (inferior and super longitudinal fasciculus). In GMV, they found reductions in the superior temporal gyrus of both hemispheres.},
author = {Steinbrink, C. and Vogt, K. and Kastrup, A. and M{\"{u}}ller, H. P. and Juengling, F. D. and Kassubek, J. and Riecker, A.},
doi = {10.1016/j.neuropsychologia.2008.07.015},
file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Steinbrink et al. - 2008 - The contribution of white and gray matter differences to developmental dyslexia Insights from DTI and VBM at.pdf:pdf},
isbn = {0028-3932 (Print)$\backslash$n0028-3932 (Linking)},
