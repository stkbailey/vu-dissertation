journal = {Brain Structure and Function},
keywords = {Brain,Brain Mapping,Brain: physiology,Humans,Image Processing, Computer-Assisted,Magnetic Resonance Imaging,Neural Pathways,Neural Pathways: physiology},
month = {oct},
