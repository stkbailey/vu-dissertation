@article{Nation2014,
abstract = {This study investigates the oral language skills of 8-year-old children with impaired reading comprehension. Despite fluent and accurate reading and normal nonverbal ability, these children are poor at understanding what they have read. Tasks tapping 3 domains of oral language, namely phonology, semantics, and morphosyntax, were administered, along with measures that skills reflect an interaction of language domains that we refer to as . Relative to control children matched for age and decoding ability, poor comprehenders were impaired across all measures except those tapping phono- logical skills. In addition to low oral language ability characterizing the group as broader language a whole, some individuals had marked language impairments; it is argued that a substantial minority can be classified as having specific language impairment. However, none of the children had been previously recognized as having a language or reading impairment. These findings demonstrate that serious reading and language impairments are not always obvious in children who have good phonological ability and appear, superficially at least, to read well.},
annote = {This study investigates the differences between poor comprehenders and TD controls (n=22 and 23) on a variety of language tasks with the overarching goal of identifying whether RCD kids would be identified as having SLI. (That is, had they been referred from the classroom; RCD is usually not identified.) To do this, they measured ability in phonology, semantics and morphosynta, along with "broader language skills". RCD was similar to TD in phonology but worse in all other domains. The authors determined that many of the RCD subjects would qualify as having SLI, and about 50{\%} of those that would qualify also had a 15 pt standard score difference between language competency and nonverbal ability. Thus, while RCD children do not have decoding or speaking issues, they do have semantic and morphosyntactic issues that are hindered in SLI. There is thus evidence for overlap between these conditions.},
author = {Nation, Kate and Clarke, Paula J. and Marshall, Catherine M. and Durand, Marianne},
doi = {10.1044/1092-4388(2004/017)},
file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Nation et al. - 2004 - Hidden Language Impairments in Children Parallels Between Poor Reading Comprehension and Specific Language Imp(2).pdf:pdf},
