journal = {Journal of Cognitive Neuroscience},
keywords = {Acoustic Stimulation,Acoustic Stimulation: methods,Adolescent,Attention,Attention Deficit Disorder with Hyperactivity,Attention Deficit Disorder with Hyperactivity: eti,Attention: physiology,Child,Dyslexia,Dyslexia: complications,Female,Humans,Male,Mental Processes,Mental Processes: physiology,Neuropsychological Tests,Phonetics,Photic Stimulation,Photic Stimulation: methods,Reaction Time,Reaction Time: physiology,Regression Analysis,Space Perception,Space Perception: physiology,Time Factors},
