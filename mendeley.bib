@article{Humphreys2014,
abstract = {Despite much interest in language production and comprehension mechanisms, little is known about the relationship between the two. Previous research suggests that linguistic knowledge is shared across these tasks and that the left inferior frontal gyrus (LIFG) may be commonly recruited. However, it remains unclear the extent to which production and comprehension share competition mechanisms. Here we investigate this issue and specifically examine competition in determining the event roles in a sentence (agent or affected participant). We used both behavioral and fMRI methods and compared the reading and production of high- and low-competition sentences, specifically targeting LIFG. We found that activity in pars opercularis (PO), independently identified by a competition-driven localizer, was modulated by competition in both tasks. Psychophysiological interaction analyses seeded in PO revealed task-specific networks: In comprehension, PO only interacted with the posterior temporal lobe, whereas in production, it interacted with a large network including hippocampal, posterior temporal, medial frontal and subcortical structures. Production and comprehension therefore recruit partially distinct functional networks but share competitive processes within fronto-temporal regions. We argue that these common regions store long-term linguistic associations and compute their higher-order contingencies, but competition in production ignites a larger neural network implementing planning, as required by task demands. {\textcopyright} 2013.},
author = {Humphreys, Gina F. and Gennari, Silvia P.},
doi = {10.1016/j.neuroimage.2013.08.059},
file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Humphreys, Gennari - 2014 - Competitive mechanisms in sentence processing Common and distinct production and reading comprehension netwo.pdf:pdf},
isbn = {1095-9572 (Electronic)$\backslash$r1053-8119 (Linking)},
issn = {10538119},
journal = {NeuroImage},
keywords = {Competition,Interactive networks,LIFG,Language processing,Language production and comprehension,Production planning},
month = {jan},
pages = {354--366},
pmid = {24012545},
publisher = {Elsevier Inc.},
title = {{Competitive mechanisms in sentence processing: Common and distinct production and reading comprehension networks linked to the prefrontal cortex}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24012545},
volume = {84},
year = {2014}
}
@article{Linkersdorfer2012,
abstract = {The neural correlates of developmental dyslexia have been investigated intensively over the last two decades and reliable evidence for a dysfunction of left-hemispheric reading systems in dyslexic readers has been found in functional neuroimaging studies. In addition, structural imaging studies using voxel-based morphometry (VBM) demonstrated grey matter reductions in dyslexics in several brain regions. To objectively assess the consistency of these findings, we performed activation likelihood estimation (ALE) meta-analysis on nine published VBM studies reporting 62 foci of grey matter reduction in dyslexic readers. We found six significant clusters of convergence in bilateral temporo-parietal and left occipito-temporal cortical regions and in the cerebellum bilaterally. To identify possible overlaps between structural and functional deviations in dyslexic readers, we conducted additional ALE meta-analyses of imaging studies reporting functional underactivations (125 foci from 24 studies) or overactivations (95 foci from 11 studies ) in dyslexics. Subsequent conjunction analyses revealed overlaps between the results of the VBM meta-analysis and the meta-analysis of functional underactivations in the fusiform and supramarginal gyri of the left hemisphere. An overlap between VBM results and the meta-analysis of functional overactivations was found in the left cerebellum. The results of our study provide evidence for consistent grey matter variations bilaterally in the dyslexic brain and substantial overlap of these structural variations with functional abnormalities in left hemispheric regions.},
annote = {There is reliable evidence for dysfunction of left hemisphere reading regions in functional neuroimaging and in studies of gray matter density. A study of overlap between these structural and functional findings. Performing a meta-analysis of nine structural studies and 24 functional ones, they identified structural differences in bilateral temporo-parietal areas and left occiptio-temporal areas in gray matter. They found that these overlapped with functional differences - underactivations, specifically - in the fusiform and supramargina gyri of the left hemisphere. There were also functional overactivation overlaps in the left cerebellum.},
author = {Linkersd??rfer, Janosch and Lonnemann, Jan and Lindberg, Sven and Hasselhorn, Marcus and Fiebach, Christian J.},
doi = {10.1371/journal.pone.0043122},
file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Linkersdrfer et al. - 2012 - Grey matter alterations co-localize with functional abnormalities in developmental dyslexia An ALE meta-ana.pdf:pdf},
isbn = {1932-6203 (Electronic)$\backslash$n1932-6203 (Linking)},
issn = {19326203},
journal = {PLoS ONE},
keywords = {Brain,Brain: pathology,Dyslexia,Dyslexia: pathology,Humans,Likelihood Functions},
month = {jan},
number = {8},
pages = {e43122},
pmid = {22916214},
title = {{Grey matter alterations co-localize with functional abnormalities in developmental dyslexia: An ALE meta-analysis}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3423424{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {7},
year = {2012}
}
