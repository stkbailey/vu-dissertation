number = {4},
pages = {203--212},
title = {{Critical Elements of Classroom and Small-Group Instruction Promote Reading Success in All Children}},
url = {http://doi.wiley.com/10.1111/0938-8982.00020},
volume = {16},
year = {2001}
}
@article{Elleman2009,
abstract = {A meta-analysis of vocabulary interventions in grades pre-K to 12 was conducted with 37 studies to better understand the impact of vocabulary on comprehension. Vocabulary instruction was found to be effective at increasing students' ability to comprehend text with custom measures (d = 0.50), but was less effective for standardized measures (d = 0.10). When considering only custom measures, and controlling for method variables, students with reading difficulties (d = 1.23) benefited more than three times as much as students without reading problems (d = 0.39) on comprehension measures. Gains on vocabulary measures, however, were comparable across reading ability. In addition, the correlation of vocabulary and comprehension effects from studies reporting both outcomes was modest (r = .43).},
author = {Elleman, Amy M. and Lindo, Endia J. and Morphy, Paul and Compton, Donald L.},
doi = {10.1080/19345740802539200},
file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Elleman et al. - 2009 - The Impact of Vocabulary Instruction on Passage-Level Comprehension of School-Age Children A Meta-Analysis.pdf:pdf},
isbn = {1934-5747},
issn = {1934-5747},
journal = {Journal of Research on Educational Effectiveness},
month = {jan},
number = {1},
pages = {1--44},
title = {{The Impact of Vocabulary Instruction on Passage-Level Comprehension of School-Age Children: A Meta-Analysis}},
url = {http://www.tandfonline.com/doi/abs/10.1080/19345740802539200},
volume = {2},
year = {2009}
}
@book{Ehri1998,
abstract = {When students attain reading skill, they learn to read words in several ways. Familiar words are read by sight. Unfamiliar words are read by decoding, by analogy to known words, or by prediction from graphophonic and contextual cues. Five phases of development are identified to distinguish the course of word reading; each phase is characterized by students' working knowledge of the alphabetic system, which is central for acquiring word reading skill. The phases are: pre‐alphabetic, partial alphabic, full alphabetic, consolidated alphabetic, and automatic alphabetic. The usefulness of this information for teachers of problem readers is explained.},
author = {Ehri, Linnea C. and McCormick, Sandra},
booktitle = {Reading and Writing Quarterly},
doi = {10.1080/1057356980140202},
file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ehri, McCormick - 1998 - Phases of word learning Implications for instruction with delayed and disabled readers.pdf:pdf},
