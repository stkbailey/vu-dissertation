journal = {NeuroImage},
keywords = {Adolescent,Adult,Anisotropy,Brain,Brain: growth {\&} development,Brain: physiology,Child,Computer-Assisted,Diffusion Magnetic Resonance Imaging,Female,Humans,Image Processing,Male,Reading},
