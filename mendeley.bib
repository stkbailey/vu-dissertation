doi = {10.1073/pnas.1322173111},
eprint = {arXiv:1604.05974v2},
file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Warren et al. - 2014 - Network measures predict neuropsychological outcome after brain injury.pdf:pdf},
isbn = {1091-6490 (Electronic)$\backslash$r0027-8424 (Linking)},
