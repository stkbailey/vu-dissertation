file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Cheng et al. - 2015 - White matter abnormalities of microstructure and physiological noise in schizophrenia.pdf:pdf},
isbn = {1931-7557},
issn = {19317565},
journal = {Brain Imaging and Behavior},
keywords = {Physiological noise,Resting state fMRI,Schizophrenia,White matter},
month = {jan},
number = {4},
pages = {868--877},
pmid = {25560665},
title = {{White matter abnormalities of microstructure and physiological noise in schizophrenia}},
