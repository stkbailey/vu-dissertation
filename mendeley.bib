journal = {Journal of Experimental Child Psychology},
keywords = {anne castles and,behavioral genetics,by national institutes of,dyslexia,genetic influences,hd11683 to richard olson,health grants hd22223 and,orthographic deficits,phonological deficits,questions,reading disorders,research council grant to,subtypes of dyslexia,supported by an australian,the present research was},
number = {2},
pages = {73--94},
pmid = {9927524},
title = {{Varieties of Developmental Reading Disorder: Genetic and Environmental Influences}},
