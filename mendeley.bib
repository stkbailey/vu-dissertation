keywords = {Adult,Algorithms,Brain,Brain: anatomy {\&} histology,Brain: physiology,Female,Fractals,Humans,Image Processing, Computer-Assisted,Image Processing, Computer-Assisted: methods,Magnetic Resonance Imaging,Male,Reproducibility of Results,Young Adult},
