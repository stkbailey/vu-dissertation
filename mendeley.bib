file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Norton et al. - 2014 - Functional neuroanatomical evidence for the double-deficit hypothesis of developmental dyslexia.pdf:pdf},
isbn = {1873-3514 (Electronic)$\backslash$r0028-3932 (Linking)},
issn = {18733514},
journal = {Neuropsychologia},
keywords = {Developmental dyslexia,Double deficit,FMRI,Phonological processing,Reading},
