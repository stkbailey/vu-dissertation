journal = {PLoS ONE},
keywords = {Anisotropy,Brain,Brain: anatomy {\&} histology,Brain: growth {\&} development,Child,Diffusion Tensor Imaging,Diffusion Tensor Imaging: methods,Female,Humans,Male},
month = {jan},
