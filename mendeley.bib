file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Fair et al. - 2007 - Development of distinct control networks through segregation and integration.pdf:pdf},
isbn = {0027-8424 (Print)$\backslash$r0027-8424 (Linking)},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences},
keywords = {Adolescent,Adult,Cerebellum,Cerebellum: physiology,Child,Humans,Magnetic Resonance Imaging,Nerve Net,Nerve Net: drug effects,Oxygen,Oxygen: blood},
month = {aug},
number = {33},
pages = {13507--13512},
pmid = {17679691},
title = {{Development of distinct control networks through segregation and integration}},
url = {http://www.pnas.org/cgi/doi/10.1073/pnas.0705843104},
