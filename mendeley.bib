isbn = {1471-003X},
issn = {1471-003X},
journal = {Nature Reviews Neuroscience},
keywords = {Adaptation,Animals,Brain,Brain: cytology,Brain: physiology,Humans,Mental Disorders,Mental Disorders: metabolism,Mental Disorders: pathology,Mental Disorders: physiopathology,Nerve Net,Nerve Net: cytology,Nerve Net: physiology,Nervous System Diseases,Nervous System Diseases: metabolism,Nervous System Diseases: pathology,Nervous System Diseases: physiopathology,Neural Pathways,Neural Pathways: cytology,Neural Pathways: physiology,Physiological,Physiological: physiology},
month = {may},
number = {5},
pages = {336--49},
pmid = {22498897},
title = {{The economy of brain network organization}},
url = {http://www.nature.com/doifinder/10.1038/nrn3214},
volume = {13},
year = {2012}
}
@article{Lim2013,
abstract = {OBJECTIVE: Attention Deficit Hyperactivity Disorder (ADHD) is a neurodevelopmental disorder, but diagnosed by subjective clinical and rating measures. The study's aim was to apply Gaussian process classification (GPC) to grey matter (GM) volumetric data, to assess whether individual ADHD adolescents can be accurately differentiated from healthy controls based on objective, brain structure measures and whether this is disorder-specific relative to autism spectrum disorder (ASD).$\backslash$n$\backslash$nMETHOD: Twenty-nine adolescent ADHD boys and 29 age-matched healthy and 19 boys with ASD were scanned. GPC was applied to make disorder-specific predictions of ADHD diagnostic status based on individual brain structure patterns. In addition, voxel-based morphometry (VBM) analysis tested for traditional univariate group level differences in GM.$\backslash$n$\backslash$nRESULTS: The pattern of GM correctly classified 75.9{\%} of patients and 82.8{\%} of controls, achieving an overall classification accuracy of 79.3{\%}. Furthermore, classification was disorder-specific relative to ASD. The discriminating GM patterns showed higher classification weights for ADHD in earlier developing ventrolateral/premotor fronto-temporo-limbic and stronger classification weights for healthy controls in later developing dorsolateral fronto-striato-parieto-cerebellar networks. Several regions were also decreased in GM in ADHD relative to healthy controls in the univariate VBM analysis, suggesting they are GM deficit areas.$\backslash$n$\backslash$nCONCLUSIONS: The study provides evidence that pattern recognition analysis can provide significant individual diagnostic classification of ADHD patients and healthy controls based on distributed GM patterns with 79.3{\%} accuracy and that this is disorder-specific relative to ASD. Findings are a promising first step towards finding an objective differential diagnostic tool based on brain imaging measures to aid with the subjective clinical diagnosis of ADHD.},
annote = {This paper, similarly to our own SRCD-DYS-TD paper, uses an MVPA classifier to differentiate between ADHD, TD and Autistic groups with T1w structural data. They do all the two-way comparisons as well as a three-way comparison. They use a Gaussian Process Classifier rather than an SVM. Their sample sizes are: 29 control, 29 ADHD, 29 ASD. The classifier achieves 79{\%} accuracy between ADHD-td,77.1{\%} between ADHD-(TD|ASD), 85.2{\%} between ADHD-ASD and 68.2{\%} between 3-way ADHD-ASD-TD. They also cite fronto-striatal, fronto-cerebellar and temporo-parietal areas as different, including the cerebellum.Finally, overlapping univariate differences were found in many regions.

This research comes in light of a disappointing result from the multi-site FCON ADHD-200 experiment which sought to identify classifiers of ADHD across multiple imaging sites. The best classifiers ended up having an accuracy of 58{\%}. The authors state the results obtained here do show the feasibility of finding differences between ADHD-non-ADHD brains.},
author = {Lim, Lena and Marquand, Andre and Cubillo, Ana A. and Smith, Anna B. and Chantiluke, Kaylita and Simmons, Andrew and Mehta, Mitul and Rubia, Katya},
doi = {10.1371/journal.pone.0063660},
