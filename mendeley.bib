file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Lallier et al. - 2009 - Auditory and visual stream segregation in children and adults An assessment of the amodality assumption of the '.pdf:pdf},
isbn = {1872-6240 (Electronic)$\backslash$r0006-8993 (Linking)},
issn = {00068993},
journal = {Brain Research},
keywords = {Audition,Dyslexia,Reading,Sluggish attentional shifting,Stream segregation,Vision},
pages = {132--147},
pmid = {19631618},
publisher = {Elsevier B.V.},
title = {{Auditory and visual stream segregation in children and adults: An assessment of the amodality assumption of the 'sluggish attentional shifting' theory of dyslexia}},
url = {http://dx.doi.org/10.1016/j.brainres.2009.07.037},
