journal = {Journal of Neuroscience},
keywords = {alluvial diagram,and hence was,default mode network,dmn,during goal-directed tasks,flexibility,functional connectivity,graph theory,has been shown to,historically assumed to disengage,however,implicates the dmn in,increase its activity during,large-scale brain network,recent evidence,self-referential,significance statement,stimulation,the absence of external,the default mode network},
number = {46},
pages = {15254--15262},
pmid = {26586814},
title = {{Default Mode Dynamics for Global Functional Integration}},
