journal = {PLoS ONE},
number = {9},
pmid = {23028644},
title = {{Resting-State Functional Connectivity Patterns Predict Chinese Word Reading Competency}},
volume = {7},
year = {2012}
}
@article{Zhang2014,
abstract = {An intriguing discovery in recent years is that resting-state functional connectivity (RSFC) is associated with cognitive performance. The current study investigated whether RSFC within the reading network was correlated with Chinese adults' reading abilities in their native language (L1, Chinese) and second language (L2, English). Results showed that RSFC within the reading network was positively correlated to reading abilities in L1 and L2, and RSFC between reading areas and the default network was negatively correlated to reading abilities in L1 and L2. Further conjunction and contrast analyses revealed that L1 and L2 shared similar RSFC correlates including connectivities between the areas for visual analysis (e.g., bilateral posterior fusiform gyrus, lateral occipital cortices, and right superior parietal lobules) and those for phonological processing (e.g., bilateral precentral gyri and postcentral gyrus, Wernicke's area). These results indicate that RSFC is a potential neural marker for reading abilities in both L1 and L2, with important theoretical implications for reading in L1 and L2. {\textcopyright} 2013 Elsevier Inc.},
author = {Zhang, Mingxia and Li, Jin and Chen, Chuansheng and Xue, Gui and Lu, Zhonglin and Mei, Leilei and Xue, Hongli and Xue, Feng and He, Qinghua and Chen, Chunhui and Wei, Miao and Dong, Qi},
