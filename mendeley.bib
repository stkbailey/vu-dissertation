@article{Davis2013,
abstract = {Impulsivity is a complex trait associated with a range of maladaptive behaviors, including many forms of psychopathology. Previous research has implicated multiple neural circuits and neurotransmitter systems in impulsive behavior, but the relationship between impulsivity and organization of whole-brain networks has not yet been explored. Using graph theory analyses, we characterized the relationship between impulsivity and the functional segregation ("modularity") of the whole-brain network architecture derived from resting-state functional magnetic resonance imaging (fMRI) data. These analyses revealed remarkable differences in network organization across the impulsivity spectrum. Specifically, in highly impulsive individuals, regulatory structures including medial and lateral regions of the prefrontal cortex were isolated from subcortical structures associated with appetitive drive, whereas these brain areas clustered together within the same module in less impulsive individuals. Further exploration of the modular organization of whole-brain networks revealed novel shifts in the functional connectivity between visual, sensorimotor, cortical, and subcortical structures across the impulsivity spectrum. The current findings highlight the utility of graph theory analyses of resting-state fMRI data in furthering our understanding of the neurobiological architecture of complex behaviors.},
annote = {This article invesitgates how the modularity of resting-state networks change as a function of impulsivity. Specificially, they use the correlations between different seed regions throughout the brain to identify "modules", (namely, visual, cortical control, subcortical, sensorimotor modules) that are more or less coupled throughout a resting-state scan. Their central finding is that "decreased functional coupl9ing between cortical control and subcotical drive modules as a function of increasing impulsivity". While much of their interpretation is speculative, they use two methods that are of interest: first, their identification of modules could be useful and second, their use of "time windows" to look at how different modules inter-relate over time, as a function of impulsivity. These methods, in connection to our fMRI studies, could be useful.},
author = {Davis, F. Caroline and Knodt, Annchen R. and Sporns, Olaf and Lahey, Benjamin B. and Zald, David H. and Brigidi, Bart D. and Hariri, Ahmad R.},
doi = {10.1093/cercor/bhs126},
file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Davis et al. - 2013 - Impulsivity and the modular organization of resting-state neural networks.pdf:pdf},
isbn = {1460-2199 (Electronic)$\backslash$n1047-3211 (Linking)},
issn = {10473211},
journal = {Cerebral Cortex},
keywords = {Barratt Impulsiveness Scale,functional connectivity,graph theory,neuroimaging,resting-state fMRI},
month = {jun},
number = {6},
pages = {1444--1452},
pmid = {22645253},
title = {{Impulsivity and the modular organization of resting-state neural networks}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22645253},
volume = {23},
year = {2013}
}
