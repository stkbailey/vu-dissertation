file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Hrdlicka et al. - 2005 - Subtypes of autism by cluster analysis based on structural MRI data.pdf:pdf},
isbn = {10188827},
