journal = {Proceedings of the National Academy of Sciences},
keywords = {Brain Mapping,Case-Control Studies,Cerebral Cortex,Cerebral Cortex: abnormalities,Child,China,Dyslexia,Dyslexia: etiology,Dyslexia: pathology,Female,Humans,Language,Magnetic Resonance Imaging,Male,Reading},
