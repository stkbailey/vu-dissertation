eprint = {NIHMS150003},
file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Finn et al. - 2014 - Disruption of functional networks in dyslexia A whole-brain, data-driven analysis of connectivity(2).pdf:pdf},
isbn = {1873-2402 (Electronic)$\backslash$r0006-3223 (Linking)},
