pmid = {23395846},
title = {{Knockdown of the dyslexia-associated gene Kiaa0319 impairs temporal responses to speech stimuli in rat primary auditory cortex}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23395846},
volume = {24},
year = {2014}
