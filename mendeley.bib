eprint = {NIHMS150003},
file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Thomas et al. - 2009 - Functional but not structural changes associated with learning An exploration of longitudinal Voxel-Based Morphom.pdf:pdf},
isbn = {1095-9572 (Electronic)$\backslash$n1053-8119 (Linking)},
issn = {10538119},
journal = {NeuroImage},
keywords = {Adult,Brain,Brain Mapping,Brain Mapping: methods,Brain: anatomy {\&} histology,Brain: physiology,Computer-Assisted,Female,Humans,Learning,Learning: physiology,Magnetic Resonance Imaging,Magnetic Resonance Imaging: methods,Male,Neuropsychological Tests,Psychomotor Performance,Psychomotor Performance: physiology,Signal Processing,Young Adult},
