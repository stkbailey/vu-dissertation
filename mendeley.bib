journal = {NeuroImage},
keywords = {Multivariate pattern analysis,Resting-state,Sex difference,Structure-function relationship},
number = {4},
pages = {931--940},
pmid = {22498657},
publisher = {Elsevier Inc.},
title = {{Combined structural and resting-state functional MRI analysis of sexual dimorphism in the young adult human brain: An MVPA approach}},
