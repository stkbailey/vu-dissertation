eprint = {arXiv:0811.2183v2},
file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Mahone et al. - 2002 - Validity of the behavior rating inventory of executive function in children with ADHD andor Tourette syndrome.pdf:pdf},
