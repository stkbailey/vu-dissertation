journal = {NeuroImage},
keywords = {Adult,Diagnostic Techniques, Neurological,Diagnostic Techniques, Neurological: statistics {\&},Humans,Magnetic Resonance Imaging,Male,Models, Statistical,Prospective Studies,Schizophrenia,Schizophrenia: classification,Schizophrenia: diagnosis,Software},
