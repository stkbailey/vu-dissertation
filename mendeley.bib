isbn = {2168-6238 (Electronic)$\backslash$r2168-622X (Linking)},
issn = {2168-622X},
journal = {JAMA Psychiatry},
keywords = {Adolescent,Adult,Antidepressive Agents, Second-Generation,Antidepressive Agents, Second-Generation: administ,Antidepressive Agents, Second-Generation: pharmaco,Biological Markers,Biological Markers: metabolism,Brain Chemistry,Brain Chemistry: drug effects,Brain Chemistry: physiology,Citalopram,Citalopram: administration {\&} dosage,Citalopram: pharmacology,Cognitive Therapy,Cognitive Therapy: methods,Combined Modality Therapy,Depressive Disorder, Major,Depressive Disorder, Major: drug therapy,Depressive Disorder, Major: metabolism,Depressive Disorder, Major: therapy,Female,Humans,Male,Middle Aged,Neuroimaging,Neuroimaging: methods,Positron-Emission Tomography,Positron-Emission Tomography: instrumentation,Positron-Emission Tomography: methods,Remission Induction,Treatment Outcome},
