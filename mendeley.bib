issn = {01497634},
journal = {Neuroscience and Biobehavioral Reviews},
keywords = {Arcuate fasciculus,Corona radiate,Corpus callosum,Diffusion tensor imaging,Dyslexia,Fibertracking,Fractional anisotropy,Reading,Review},
month = {jul},
number = {6},
