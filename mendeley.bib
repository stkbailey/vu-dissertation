@article{Catts2003,
abstract = {The present study investigated the use of the Reading Component Model to subgroup poor readers. A large sample of poor readers was identified in second grade and subgrouped on the basis of relative strengths and weaknesses in word recognition and listening comprehension. Although homogeneous subgroups were not identified, poor readers could be classified into four subgroups that differed significantly in reading-related abilities. Further analyses showed that poor readers' strengths and weaknesses in listening comprehension, and to a lesser extent in word recognition, were foreshadowed by their abilities on related kindergarten measures. Follow-up testing in the fourth grade indicated that poor readers' individual differences in word recognition and listening comprehension were consistent and that subgroups were moderately stable. The implications of these results for the assessment and remediation of reading disabilities are discussed. The use of IQ-achievement discrepancy to classify poor readers has played a central role in the field of reading and learning disabilities. In this approach, poor readers are subgrouped into those displaying a discrepancy between IQ and reading achievement scores and those who do not show such a discrepancy. The former have often been referred to as children with dyslexia or specific reading disability, and the latter are frequently labeled slow learners or garden-variety poor readers. Whereas classification based on IQ-achievement discrepancy has been widespread, its validity and utility have been challenged, especially in recent years (Aaron, 1991, 1997; Fletcher et al., 1998; Siegel, 1989; Stanovich, 1991).},
annote = {The authors applied the "Reading Component Model" based on the Simple View to a group of 200 school age children and investigated how well the model fit their observed reading comprehension impairments. Second grade scores were focused on ut also extended to the Kindergarten and fourth grades. Generally, they found the component view to fit the sample well, with children's group classificatino being relatively consistent (about 2/3 remain the same) into fourth grade. Unexpectedly, hyperlexic (or specifcially poor comprehenders) showed phonological deficits in both Kindergarten and second grade, despite adequate word recognition. A fourth subgroup, non-specifiied reading disabilities had to be created to explain all data, but the authors state that this could be an artifact of the data, given that the boundaries imposed for the model were arbitrary. The authors conclude that the reading components model is a useful and fitting model for explaining and intervening in reading disability.},
archivePrefix = {arXiv},
arxivId = {NIHMS150003},
author = {Catts, Hugh W and Hogan, Tiffany and Fey, Marc E},
doi = {10.1177/002221940303600208},
eprint = {NIHMS150003},
file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Catts, Hogan, Fey - 2003 - Subgrouping poor readers on the basis of individual differences in reading-related abilities.pdf:pdf},
isbn = {0022-2194},
issn = {0022-2194},
journal = {Journal of Learning Disabilities},
month = {mar},
number = {2},
pages = {151--164},
pmid = {15493430},
title = {{Subgrouping poor readers on the basis of individual differences in reading-related abilities}},
url = {http://ldx.sagepub.com/cgi/doi/10.1177/002221940303600208},
volume = {36},
year = {2003}
}
@article{Aaron1999,
abstract = {In this article, reading disability is defined broadly to refer to below-average achievement in reading comprehension as assessed by a standardized test. With our research we tried to answer the question of whether all children with reading disability share a common etiology of deficient phonology, or consitute heterogeneous groups. The answer to this question was sought in four studies that examiend reading disabilities from the perspective of componential skills of reading.},
annote = {In four parts , the authors propose and support a componential model of reading (the Reading Components model), based on the simple view. First, they perform a PCA on 200 students' data and find two factors in 3rd grade which correspond to Word Recognition and Language Comprehension. In 6th grade PCA, a reading speed factor is also found. Secnd, they investigate two groups of children to find the distribution of reading disorders (i.e. decoding only, comprehension only, both). They found that in the unbiased group, there was roughly equal distribution, but in a Title I child group, there were greater comprehension deficits. This is notable because children in learning disabilities programs typically have higher IQ than children in Title I programs, and only one Title I chidl by the IQ-reading dissociation criteria fit the label for developmental dyslexia. In the fourth study, they examine individuals in LD classroom and find that most of their weaknesses are in decoding. Thus, generalizing from this population will skew our understanding of reading disorders. They conclude that the distribution of reader subtypes will vary dependin on the group under study.},
author = {Aaron, P.G. and Joshi, M. and Williams, K. A.},
doi = {10.1177/002221949903200203},
file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Aaron, Joshi, Williams - 1999 - Not All Reading Disabilities Are Alike.pdf:pdf},
isbn = {0022-2194 (Print)$\backslash$r0022-2194 (Linking)},
issn = {0022-2194},
journal = {Journal of Learning Disabilities},
number = {2},
pages = {120--137},
pmid = {15499713},
title = {{Not All Reading Disabilities Are Alike}},
url = {http://ldx.sagepub.com/cgi/doi/10.1177/002221949903200203},
volume = {32},
year = {1999}
}
@article{Horwitz2003,
abstract = {Neurons and neural populations do not function as islands onto themselves. Rather, they interact with other such elements through their afferent and efferent connections in an orchestrated manner so as to enable different sensorimotor and cognitive tasks to be performed. The concept of functional connectivity and the allied notion of effective connectivity were introduced to designate the functional strengths of such interactions. Functional neuroimaging methods, especially PET and fMRI, have been used extensively to evaluate the functional connectivity between different brain regions. After providing a brief historical review of these notions of brain connectivity, I argue that the conceptual formulations of functional and effective connectivity are far from clear. Specifically, the terms functional and effective connectivity are applied to quantities computed on types of functional imaging data (e.g., PET, fMRI, EEG) that vary in spatial, temporal, and other features, using different definitions (even for data of the same modality) and employing different computational algorithms. Until it is understood what each definition means in terms of an underlying neural substrate, comparisons of functional and/or effective connectivity across studies may appear inconsistent and should be performed with great caution. {\textcopyright} 2003 Elsevier Science (USA). All rights reserved.},
author = {Horwitz, Barry},
doi = {10.1016/S1053-8119(03)00112-5},
file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Horwitz - 2003 - The elusive concept of brain connectivity.pdf:pdf},
isbn = {1053-8119 (Print)$\backslash$n1053-8119 (Linking)},
issn = {10538119},
journal = {NeuroImage},
month = {jun},
number = {2},
pages = {466--470},
pmid = {12814595},
title = {{The elusive concept of brain connectivity}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S1053811903001125},
volume = {19},
year = {2003}
}
@article{Mar2011,
abstract = {A great deal of research exists on the neural basis of theory-of-mind (ToM) or mentalizing. Qualitative reviews on this topic have identified a mentalizing network composed of the medial prefrontal cortex, posterior cingulate/precuneus, and bilateral temporal parietal junction. These conclusions, however, are not based on a quantitative and systematic approach. The current review presents a quantitative meta-analysis of neuroimaging studies pertaining to ToM, using the activation-likelihood estimation (ALE) approach. Separate ALE meta-analyses are presented for story-based and nonstory-based studies of ToM. The conjunction of these two meta-analyses reveals a core mentalizing network that includes areas not typically noted by previous reviews. A third ALE meta-analysis was conducted with respect to story comprehension in order to examine the relation between ToM and stories. Story processing overlapped with many regions of the core mentalizing network, and these shared regions bear some resemblance to a network implicated by a number of other processes.},
author = {Mar, Raymond A.},
doi = {10.1146/annurev-psych-120709-145406},
file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Mar - 2011 - The Neural Bases of Social Cognition and Story Comprehension.pdf:pdf},
