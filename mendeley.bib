arxivId = {NIHMS150003},
author = {Ecker, Christine and Rocha-Rego, Vanessa and Johnston, Patrick and Mourao-Miranda, Janaina and Marquand, Andre and Daly, Eileen M. and Brammer, Michael J. and Murphy, Clodagh and Murphy, Declan G.},
doi = {10.1016/j.neuroimage.2009.08.024},
eprint = {NIHMS150003},
file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ecker et al. - 2010 - Investigating the predictive value of whole-brain structural MR scans in autism A pattern classification approach.pdf:pdf},
isbn = {1053-8119},
