isbn = {1053-8119},
issn = {10959572},
journal = {NeuroImage},
keywords = {Cortical thickness,Dyslexia,Graph theory,Structural network,Surface area},
pages = {63--74},
pmid = {26363349},
publisher = {Elsevier Inc.},
title = {{More bilateral, more anterior: Alterations of brain organization in the large-scale structural network in Chinese dyslexia}},
