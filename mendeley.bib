@article{Locascio2010,
abstract = {Emerging research supports the contribution of executive function (EF) to reading comprehension; however, a unique pattern has not been established for children who demonstrate comprehension difficulties despite average word recognition ability (specific reading comprehension deficit; S-RCD). To identify particular EF components on which children with S-RCD struggle, a range of EF skills was compared among 86 children, ages 10 to 14, grouped by word reading and comprehension abilities: 24 average readers, 44 with word recognition deficits (WRD), and 18 S-RCD. An exploratory principal components analysis of EF tests identified three latent factors, used in subsequent group comparisons: Planning/ Spatial Working Memory, Verbal Working Memory, and Response Inhibition. The WRD group exhibited deficits (relative to controls) on Verbal Working Memory and Inhibition factors; S-RCD children performed more poorly than controls on the Planning factor. Further analyses suggested the WRD group's poor performance on EF factors was a by-product of core deficits linked to WRD (after controlling for phonological processing, this group no longer showed EF deficits). In contrast, the S-RCD group's poor performance on the planning component remained significant after controlling for phonological processing. Findings suggest reading comprehension difficulties are linked to executive dysfunction; in particular, poor strategic planning/organizing may lead to reading comprehension problems.},
annote = {The authors examined the executive function abilities of children that were typically developing, had word recognition deficitis and specific readign comprehension deficits in an effort to determine patterns of EF abilitiy relative to each other. They used multiple tasks, including measures of verbal working memory, strategic planning and response inhibition. They hypothesized that children with WRD would show deficits in executive function that were tied at least in part to their pohonlogical processing deficit - i.e. this deficit resulted in impaired EF and RC. In the SCD group, they expectd to find more 'pure' EF disabilities that did not arise from these phonological processing difficulties. Their results showed differing patterns of EF disability in the two groups. The WRD group had pronounced deficits in verabl working memory and response inhibition tasks, although phonological processing accounted for some of this impairment. But RC is still reliant on EF ability distinct from these: children with SCD had strategic planning impairments. Thus, EF contributes to the development of RC.},
author = {Locascio, Gianna and Mahone, Em Mark and Eason, Sarah H Sh and Cutting, Le Laurie E},
doi = {10.1177/0022219409355476.Executive},
file = {:C$\backslash$:/Users/bailesk1/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Locascio et al. - 2015 - Executive dysfunction among children with reading comprehension deficits.pdf:pdf},
isbn = {0022-2194, 0022-2194},
issn = {1538-4780},
journal = {Journal of Learning Disabilities},
keywords = {EF,Specific Reading Comprehension Deficits,adhd,comprehension,dyslexia,executive function,reading,working memory},
mendeley-tags = {EF,Specific Reading Comprehension Deficits},
