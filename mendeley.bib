keywords = {Auditory Pathways,Auditory Pathways: physiology,Brain Mapping,Comprehension,Comprehension: physiology,Dominance, Cerebral,Dominance, Cerebral: physiology,Frontal Lobe,Frontal Lobe: physiology,Humans,Nerve Net,Nerve Net: physiology,Phonetics,Semantics,Speech Acoustics,Speech Perception,Speech Perception: physiology,Temporal Lobe,Temporal Lobe: physiology},
month = {may},
