title = {{What Reading Does for the Mind}},
volume = {22},
year = {1998}
}
@article{Thomas2009,
abstract = {Voxel-Based Morphometry (VBM) has been used for several years to study differences in brain structure between populations. Recently, a longitudinal version of VBM has been used to show changes in gray matter associated with relatively short periods of training. In the present study we use fMRI and three different standard implementations of longitudinal VBM: SPM2, FSL, and SPM5 to assess functional and structural changes associated with a simple learning task. Behavioral and fMRI data clearly showed a significant learning effect. However, initially positive VBM results were found to be inconsistent across minor perturbations of the analysis technique and ultimately proved to be artifactual. When alignment biases were controlled for and recommended statistical procedures were used, no significant changes in grey matter density were found. This work, initially intended to show structural and functional changes with learning, rather demonstrates some of the potential pitfalls of existing longitudinal VBM methods and prescribes that these tools be applied and interpreted with extreme caution.},
annote = {This study began initially as an investigation of the overlap between functional and structural changes in learning, but morphed into a compoarison of longitudinal VBM analysis packages. They used VBM2, VBM5 and FSL, and found that each method produced a variety of differeces. Although these were small N (12), each subject served as both control and testing group. The takeaways were: alignment should be to an intermdiary scan between the longitudinal sessions (i.e. RMS of time1/time2, not time2 to time1). Second, non-parametric tools should be used for correction. The most important source of variation is the segmentation. Although the authors do not cast a doubt on whether brain size can change due to learning (there are animal studies for this), they do emphasize a need for strong methods to show this.},
archivePrefix = {arXiv},
arxivId = {NIHMS150003},
author = {Thomas, Adam G. and Marrett, Sean and Saad, Ziad S. and Ruff, Douglas A. and Martin, Alex and Bandettini, Peter A.},
