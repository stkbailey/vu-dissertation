pages = {505--516},
pmid = {25827242},
title = {{Functional Connectivity in Multiple Cortical Networks Is Associated with Performance Across Cognitive Domains in Older Adults}},
url = {http://online.liebertpub.com/doi/10.1089/brain.2014.0327},
volume = {5},
